{
  "name": "eleventy-netlify-boilerplate",
  "version": "1.0.1",
  "description": "A boilerplate for building a fast static website with the Eleventy static site generator, for deployment to Netlify.",
  "scripts": {
    "build": "npx eleventy",
    "fullbuild": "gulp watch & npx eleventy --serve",
    "watch": "npx eleventy --watch",
    "debug": "DEBUG=* npx eleventy",
    "sass:tokens": "npx json-to-scss src/_data/tokens.json src/_includes/assets/scss/settings/_tokens.scss",
    "dev": "gulp watch & eleventy --serve --quiet",
    "dev:eleventy": "eleventy --serve",
    "dev:gulpcss": "gulp css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate"
  },
  "author": {
    "name": "Dan Urbanowicz",
    "email": "dan.urbanowicz@gmail.com",
    "url": "https://www.danurbanowicz.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate/issues"
  },
  "homepage": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "luxon": "^1.19.2",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.4"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "clean-css": "^4.2.1",
    "eleventy-plugin-blog-tools": "^0.2.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-dart-sass": "^0.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "html-minifier": "^4.0.0",
    "json-to-scss": "^1.4.0",
    "netlify-cli": "^2.19.0",
    "prismjs": "^1.17.1",
    "uglify-es": "^3.3.9"
  }
}
