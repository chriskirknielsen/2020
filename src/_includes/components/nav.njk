<nav>
  <ul>
  {%- for nav in collections.nav -%}
    {# {% for p,v in nav %}[{{p}} = {{v}}]<br>{% endfor %} #}
    {% set navExists = nav | navLocale(navSet, locale) %}
    {% if navExists %}
    <li>
      <a href="{{ nav.url | url }}"{% if nav.url == page.url %} data-current="true"{% endif %}>{{ nav.data.navtitle }}</a>
    </li>
    {%- endif -%}
  {%- endfor -%}
  </ul>

  {{ en.locale }}
</nav>
