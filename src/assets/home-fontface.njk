---
permalink: /assets/css/home-fontface.css
---
{% for fontPurpose, fontName in tokens.fonts.home_mark %}
@font-face {
	font-family: "{{ fontName }}";
	src: 
		url("/{{ metadata.assetUrl.fontFaceFolder }}/{{ fontName | replace(" ", "") }}-subset.woff2") format("woff2"),
		url("/{{ metadata.assetUrl.fontFaceFolder }}/{{ fontName | replace(" ", "") }}-subset.woff") format("woff");
	font-style: normal;
	font-weight: 400;
	font-display: swap;
}
{% endfor %}